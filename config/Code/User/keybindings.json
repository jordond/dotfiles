// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+shift+down", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+up", "command": "editor.action.copyLinesUpAction", "when": "editorTextFocus"
  },
  { "key": "ctrl+shift+alt+f",   "command": "eslint.fixAllProblems",
                                     "when": "editorTextFocus" }
]