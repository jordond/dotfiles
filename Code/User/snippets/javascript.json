  "Angular Directive": {
    "prefix": "ngdirective",
    "description": "Angular 1 Directive",
    "body": [
      "(function () {",
      "\t'use strict';",
      "",
      "\t/* @ngdoc directive",
      "\t * @name ${module:app}.directive:${name}",
      "\t * @restrict ${restrict:EA}",
      "\t * @element",
      "\t * <div ${element}></div>",
      "\t * <${element}></${element}>",
      "\t * @description",
      "\t * ${description}",
      "\t *",
      "\t */",
      "\tangular",
      "\t\t.module('${module:app}')",
      "\t\t.directive('${name}', ${name}Config);",
      "",
      "\t/** @ngInject */",
      "\tfunction ${name}Config() {",
      "\t\tvar directive = {",
      "\t\t\tscope: {},",
      "\t\t\ttemplateUrl: '',",
      "\t\t\trestrict: '${restrict:EA}',",
      "\t\t\treplace: false,",
      "\t\t\tcontroller: CtrlFunct,",
      "\t\t\tcontrollerAs: 'vm',",
      "\t\t\tbindToController: true,",
      "\t\t\tlink: LinkFunct",
      "\t\t};",
      "",
      "\t\treturn directive;",
      "",
      "\t\t/** @ngInject */",
      "\t\tfunction CtrlFunct() {",
      "\t\t\tvar vm = this;",
      "\t\t\tvm.name = '${name}';",
      "\t\t\t${1}",
      "\t\t}",
      "",
      "\t\tfunction LinkFunct(scope, element, attrs) {",
      "\t\t\t${2}",
      "\t\t}",
      "\t}",
      "}());",
      ""
    ]
  },
